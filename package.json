{
  "name": "aquamarine",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node tools/copyAssets",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "parcel": "parcel build src/app.ts -d dist/main.js",
    "build": "npm-run-all clean tsc parcel",
    "dev:start": "npm-run-all build start",
    "dev": "ts-node ./src/app.ts",
    "start": "nodemon --watch src/ --ignore src/**/*.spec.ts --exec ts-node src/app.ts",
    "initdb": "ts-node tools/initdb",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@types/express": "^4.17.7",
    "@types/node": "^14.6.0",
    "@types/passport": "^1.0.4",
    "bcrypt": "^4.0.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "express-session": "^1.17.1",
    "jslint": "^0.12.1",
    "mocha": "^7.2.0",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.4",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg-promise": "^10.5.6",
    "reflect-metadata": "^0.1.13",
    "tslint": "^6.1.3",
    "typeorm": "^0.2.25",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/lodash": "^4.14.159",
    "@types/morgan": "^1.9.1",
    "@types/passport-local": "^1.0.33",
    "@types/pg-promise": "^5.4.3",
    "cypress": "^5.0.0",
    "gulp": "^4.0.2",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "ts-node": "^8.10.2"
  }
}
